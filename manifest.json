{
	"manifest_version": 3,
	"name": "Easy Notes",
	"version": "1.4",
	"description": "Easiest way for taking quick notes. ",
	"author": "daoket",
	"icons": {
		"128": "icon.png"
	},
	"action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"web_accessible_resources": [{
		"matches": ["https://*/*", "http://*/*", "file://*/*"],
		"resources": ["inject.js"]
	}]
}
